<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
	<!-- Indian vehicle types with realistic characteristics -->
	<vType id="motorcycle" accel="3.0" decel="5.0" sigma="0.8" length="2.0" maxSpeed="12.0" color="0.9,0.9,0.1" guiShape="motorcycle"/>
	<vType id="auto_rickshaw" accel="2.0" decel="4.0" sigma="0.7" length="3.0" maxSpeed="8.0" color="1,0.5,0" guiShape="passenger"/>
	<vType id="car" accel="2.5" decel="4.5" sigma="0.6" length="4.5" maxSpeed="11.0" color="0.1,0.4,1" guiShape="passenger"/>
	<vType id="bus" accel="1.5" decel="3.5" sigma="0.5" length="12.0" maxSpeed="8.0" color="0.8,0.2,0.2" guiShape="bus"/>
	<vType id="truck" accel="1.2" decel="3.0" sigma="0.6" length="10.0" maxSpeed="6.0" color="0.6,0.6,0.6" guiShape="truck"/>
	<vType id="ambulance" accel="3.0" decel="5.0" sigma="0.3" length="5.5" maxSpeed="16.0" color="1,0,0" guiShape="emergency"/>
	<vType id="police" accel="2.8" decel="4.8" sigma="0.3" length="4.8" maxSpeed="15.0" color="0,0,1" guiShape="passenger"/>

	<!-- Main arterial routes -->
	<route id="r_north_south_1" edges="J1-J5 J5-J9 J9-J13"/>
	<route id="r_north_south_2" edges="J2-J6 J6-J10 J10-J14"/>
	<route id="r_north_south_3" edges="J3-J7 J7-J11 J11-J15"/>
	<route id="r_north_south_4" edges="J4-J8 J8-J12 J12-J16"/>
	
	<route id="r_south_north_1" edges="J13-J9 J9-J5 J5-J1"/>
	<route id="r_south_north_2" edges="J14-J10 J10-J6 J6-J2"/>
	<route id="r_south_north_3" edges="J15-J11 J11-J7 J7-J3"/>
	<route id="r_south_north_4" edges="J16-J12 J12-J8 J8-J4"/>
	
	<route id="r_east_west_1" edges="J1-J2 J2-J3 J3-J4"/>
	<route id="r_east_west_2" edges="J5-J6 J6-J7 J7-J8"/>
	<route id="r_east_west_3" edges="J9-J10 J10-J11 J11-J12"/>
	<route id="r_east_west_4" edges="J13-J14 J14-J15 J15-J16"/>
	
	<route id="r_west_east_1" edges="J4-J3 J3-J2 J2-J1"/>
	<route id="r_west_east_2" edges="J8-J7 J7-J6 J6-J5"/>
	<route id="r_west_east_3" edges="J12-J11 J11-J10 J10-J9"/>
	<route id="r_west_east_4" edges="J16-J15 J15-J14 J14-J13"/>
	
	<!-- Hospital routes -->
	<route id="r_hospital_1" edges="J10-H1 H1-H2"/>
	<route id="r_hospital_2" edges="J11-H2 H2-H1"/>
	<route id="r_hospital_emergency" edges="J2-J6 J6-J10 J10-H1"/>
	<route id="r_hospital_from_south" edges="J14-J10 J10-H1"/>
	<route id="r_hospital_from_north" edges="J6-J10 J10-H1"/>
	
	<!-- Residential area routes -->
	<route id="r_residential_1" edges="J5-R1 R1-J5"/>
	<route id="r_residential_2" edges="J7-R2 R2-J7"/>
	<route id="r_residential_3" edges="J9-R3 R3-J9"/>
	<route id="r_residential_4" edges="J11-R4 R4-J11"/>

	<!-- High-density Indian traffic flows -->
	<!-- Motorcycles (very common in India) -->
	<flow id="f_motorcycles_ns1" type="motorcycle" begin="0" end="36000" route="r_north_south_1" vehsPerHour="200"/>
	<flow id="f_motorcycles_ns2" type="motorcycle" begin="0" end="36000" route="r_north_south_2" vehsPerHour="180"/>
	<flow id="f_motorcycles_ns3" type="motorcycle" begin="0" end="36000" route="r_north_south_3" vehsPerHour="190"/>
	<flow id="f_motorcycles_ns4" type="motorcycle" begin="0" end="36000" route="r_north_south_4" vehsPerHour="170"/>
	
	<flow id="f_motorcycles_ew1" type="motorcycle" begin="0" end="36000" route="r_east_west_1" vehsPerHour="150"/>
	<flow id="f_motorcycles_ew2" type="motorcycle" begin="0" end="36000" route="r_east_west_2" vehsPerHour="160"/>
	<flow id="f_motorcycles_ew3" type="motorcycle" begin="0" end="36000" route="r_east_west_3" vehsPerHour="140"/>
	<flow id="f_motorcycles_ew4" type="motorcycle" begin="0" end="36000" route="r_east_west_4" vehsPerHour="130"/>
	
	<!-- Auto-rickshaws (very common in India) -->
	<flow id="f_autos_ns1" type="auto_rickshaw" begin="0" end="36000" route="r_north_south_1" vehsPerHour="80"/>
	<flow id="f_autos_ns2" type="auto_rickshaw" begin="0" end="36000" route="r_north_south_2" vehsPerHour="90"/>
	<flow id="f_autos_ew1" type="auto_rickshaw" begin="0" end="36000" route="r_east_west_1" vehsPerHour="70"/>
	<flow id="f_autos_ew2" type="auto_rickshaw" begin="0" end="36000" route="r_east_west_2" vehsPerHour="75"/>
	
	<!-- Cars -->
	<flow id="f_cars_ns1" type="car" begin="0" end="36000" route="r_north_south_1" vehsPerHour="120"/>
	<flow id="f_cars_ns2" type="car" begin="0" end="36000" route="r_north_south_2" vehsPerHour="110"/>
	<flow id="f_cars_ns3" type="car" begin="0" end="36000" route="r_north_south_3" vehsPerHour="100"/>
	<flow id="f_cars_ns4" type="car" begin="0" end="36000" route="r_north_south_4" vehsPerHour="90"/>
	
	<flow id="f_cars_ew1" type="car" begin="0" end="36000" route="r_east_west_1" vehsPerHour="100"/>
	<flow id="f_cars_ew2" type="car" begin="0" end="36000" route="r_east_west_2" vehsPerHour="95"/>
	<flow id="f_cars_ew3" type="car" begin="0" end="36000" route="r_east_west_3" vehsPerHour="85"/>
	<flow id="f_cars_ew4" type="car" begin="0" end="36000" route="r_east_west_4" vehsPerHour="80"/>
	
	<!-- Buses (public transport) -->
	<flow id="f_buses_ns1" type="bus" begin="0" end="36000" route="r_north_south_1" vehsPerHour="15"/>
	<flow id="f_buses_ns2" type="bus" begin="0" end="36000" route="r_north_south_2" vehsPerHour="20"/>
	<flow id="f_buses_ew1" type="bus" begin="0" end="36000" route="r_east_west_1" vehsPerHour="12"/>
	<flow id="f_buses_ew2" type="bus" begin="0" end="36000" route="r_east_west_2" vehsPerHour="18"/>
	
	<!-- Trucks (commercial vehicles) -->
	<flow id="f_trucks_ns1" type="truck" begin="0" end="36000" route="r_north_south_1" vehsPerHour="25"/>
	<flow id="f_trucks_ns2" type="truck" begin="0" end="36000" route="r_north_south_2" vehsPerHour="30"/>
	<flow id="f_trucks_ew1" type="truck" begin="0" end="36000" route="r_east_west_1" vehsPerHour="20"/>
	<flow id="f_trucks_ew2" type="truck" begin="0" end="36000" route="r_east_west_2" vehsPerHour="22"/>
	
	<!-- Hospital traffic -->
	<flow id="f_hospital_visitors" type="car" begin="0" end="36000" route="r_hospital_1" vehsPerHour="40"/>
	<flow id="f_hospital_visitors_2" type="car" begin="0" end="36000" route="r_hospital_2" vehsPerHour="35"/>
	
	<!-- Emergency vehicles -->
	<flow id="f_ambulance_emergency" type="ambulance" begin="10" end="36000" route="r_hospital_emergency" period="180"/>
	<flow id="f_ambulance_routine" type="ambulance" begin="30" end="36000" route="r_hospital_from_south" period="300"/>
	<flow id="f_police_patrol" type="police" begin="60" end="36000" route="r_north_south_2" period="600"/>
	
	<!-- Residential area traffic -->
	<flow id="f_residential_1" type="car" begin="10" end="36000" route="r_residential_1" vehsPerHour="30"/>
	<flow id="f_residential_2" type="car" begin="15" end="36000" route="r_residential_2" vehsPerHour="25"/>
	<flow id="f_residential_3" type="car" begin="20" end="36000" route="r_residential_3" vehsPerHour="20"/>
	<flow id="f_residential_4" type="car" begin="25" end="36000" route="r_residential_4" vehsPerHour="22"/>
</routes>